algoritmo
	declare
	numero numerico
	escreva "digite um numero"
		leia numero
	se resto(numero,5) =0 e resto(numero,3)=0 entao
		inicio
		 	escreva "o numero que voce digitou é divisivel por 5 e 3 "
		 fim
	senao
		inicio
			escreva"o nuemro que voce digitou não é divisivel por 5 e 3"
		
		fim
fim_algoritmo
