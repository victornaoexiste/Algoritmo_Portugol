algoritmo
	declare
	n1 numerico
	n2 numerico
	r numerico
	escreva "Me diga um valor"
		leia n1
	escreva "Me diga outro valor"
		leia n2
		r <- n1 + n2
		se r > 10 entao
			inicio
				escreva r, " é maior que 10"
			fim
		senao
			inicio
			escreva "o resultado não é maior que 10"
			fim
fim_algoritmoé
