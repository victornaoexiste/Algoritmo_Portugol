algoritmo
	declare
	numero numerico 
	
	root numerico
	escreva "Me diga um numero"
		leia numero
	se raiz_quadrada(numero) >= 0 entao
		inicio
			escreva "A raiz quadrada de  ", numero, "  é positiva"
		fim
	senao
		inicio
			escreva "A raiz quadrada de  ", numero, "  é negativa"
		fim
fim_algoritmo
