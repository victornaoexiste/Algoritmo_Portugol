algoritmo
declare
pesom numerico
pesog numerico
peso numerico
valor1 numerico
valor2 numerico
escreva "qual o seu peso?"
leia peso
valor1 <- peso * 0.15
valor2 <- peso * 0.20
pesom <- peso - valor2
pesog <- peso + valor1
escreva "se voce emagressesse 20% voce pesaria ", pesom, "e se engordasse 15% voce pesaria ", pesog
fim_algoritmo
