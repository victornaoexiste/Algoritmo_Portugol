algoritmo
declare
vendas numerico
salario numerico
comissao numerico
salariof numerico
escreva "Me diga qual o salário: "
leia salario
escreva "Me diga qual o valor das vendas: "
leia vendas
comissao <- (4 * vendas) / 100
salariof <- comissao + vendas
escreva "o valor que voce irá receber é", salariof
fim_algoritmo
